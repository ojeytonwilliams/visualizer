{
  "name": "@dependency-graph/parser",
  "version": "1.0.0",
  "description": "Backend service for parsing project dependencies",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "dependency",
    "parser",
    "fastify",
    "typescript",
    "ast"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "fastify": "^5.6.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.21.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.44.1"
  }
}
