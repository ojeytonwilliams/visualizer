{
  "name": "parser",
  "version": "1.0.0",
  "description": "Backend service for parsing project dependencies",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "dependency",
    "parser",
    "fastify",
    "typescript",
    "ast"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/sensible": "^6.0.3",
    "fastify": "^5.6.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "memfs": "^4.46.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.44.1",
    "vitest": "^3.2.4"
  }
}
